{
  "name": "dvm-passgen-app",
  "version": "1.0.0",
  "description": "password generator desktop app",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test speci\fied\" && exit 1",
    "start": "electron-forge start",
    "watch": "nodemon --exec electron .",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dmacisso/dvm-passgen-app",
